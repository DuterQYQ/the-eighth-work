//5
#include<stdio.h>
#define N 5
void reverse(int arr[], int n)
{
	int p = 0, q = n - 1;
	while (q - p > 0)
	{
		int temp = arr[p];
		arr[p] = arr[q];
		arr[q] = temp;
		p++;
		q--;
	}
}
int main()
{
	int arr[N] = { 20,45,65,88,94 };
	for (int i = 0; i < N; i++)
	{
		printf("%d ", arr[i]);
	}
	printf("\n");
	reverse(arr, N);
	for (int i = 0; i < N; i++)
	{
		printf("%d ", arr[i]);
	}
	return 0;
}
